<?xml version="1.0" encoding="utf-8" ?>
<otrs_config version="1.0" init="Application">
    <CVS>$Id: MinimumLogLevel.xml,v 1.1 2011/02/12 22:57:22 reb Exp $</CVS>
    <ConfigItem Name="MinimumLogLevel" Required="0" Valid="1">
        <Description Translatable="1">Set minimum loglevel. If you select 'error', just errors are logged. With 'debug' you get all logging messages.</Description>
        <Group>Framework</Group>
        <SubGroup>Core::Log</SubGroup>
        <Setting>
            <Option SelectedID="error">
                <Item Key="error">error</Item>
                <Item Key="notice">notice</Item>
                <Item Key="info">info</Item>
                <Item Key="debug">debug</Item>
            </Option>
        </Setting>
    </ConfigItem>
</otrs_config>
